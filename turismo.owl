    <!-- http://dbpedia.org/ontology#location -->

    <owl:DatatypeProperty rdf:about="http://dbpedia.org/ontology#location">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://idi.fundacionctic.org/cruzar/turismo.html#Edificio-historico"/>
                    <rdf:Description rdf:about="http://idi.fundacionctic.org/cruzar/turismo.html#Edificio-religioso"/>
                    <rdf:Description rdf:about="http://idi.fundacionctic.org/cruzar/turismo.html#Museo"/>
                    <rdf:Description rdf:about="http://idi.fundaciontic.org/cruzar/turismo#Cafeteria"/>
                    <rdf:Description rdf:about="http://idi.fundaciontic.org/cruzar/turismo#Hostal"/>
                    <rdf:Description rdf:about="http://www.disit.org/km4city/schema#Bar"/>
                    <rdf:Description rdf:about="http://www.disit.org/km4city/schema#Discotheque"/>
                    <rdf:Description rdf:about="http://www.disit.org/km4city/schema#Hotel"/>
                    <rdf:Description rdf:about="http://www.disit.org/km4city/schema#Restaurant"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment>This data propertie represents the position of a place</rdfs:comment>
    </owl:DatatypeProperty>
    


